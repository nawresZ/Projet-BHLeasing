@model ProjetBHLeasing.Models.Profil

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Profil</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="id_profil" class="control-label"></label>
                <input asp-for="id_profil" class="form-control" />
                <span asp-validation-for="id_profil" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Designation" class="control-label"></label>
                <input asp-for="Designation" class="form-control" />
                <span asp-validation-for="Designation" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="b_systeme" class="control-label"></label>
                <input asp-for="b_systeme" class="form-control" />
                <span asp-validation-for="b_systeme" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="id_user_modif" class="control-label"></label>
                <input asp-for="id_user_modif" class="form-control" />
                <span asp-validation-for="id_user_modif" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voir liste des profils</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
